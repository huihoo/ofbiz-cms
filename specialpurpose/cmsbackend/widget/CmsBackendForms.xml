	  	       <entity-options description="${catalogName}" entity-name="CmsCatalog"></entity-options>
	  	   </drop-down>
	  	</field> 
        
        <field name="author" title="${uiLabelMap.author}" ><text/></field> 
        <field name="description" title="${uiLabelMap.description}" ><text/></field> 
        <field name="content" title="${uiLabelMap.content}" ><textarea/></field> 
        
        <field name="visitTotal" title="${uiLabelMap.visitTotal}" ><display/></field> 
        <field name="shortTitle" title="${uiLabelMap.shortTitle}" ><text/></field> 
        <field name="titleColor" title="${uiLabelMap.titleColor}" ><text/></field> 
        <field name="titleImg" title="${uiLabelMap.titleImg}" ><text/></field> 
        <field use-when="!&quot;&quot;.equals(&quot;${entity.contentImg}&quot;)" name="contentImgDisplay" title="${uiLabelMap.contentImg}" ><image value="/cms/images/upload/article/${entity.contentImg}"></image></field>
        <field name="contentImg" title="${uiLabelMap.CommonUpload}" ><file/></field> 
 
        <field name="tags" title="${uiLabelMap.tags}" ><text/></field> 
        <field name="origin" title="${uiLabelMap.origin}" ><text/></field> 
        
